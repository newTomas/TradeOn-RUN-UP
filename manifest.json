{
	"manifest_version": 2,
	"name": "Initial Profile Setting",
	"description": "Скрипт для первоначальной настройки профиля стим перед использованием ботов-трейдеров.",
	"version": "0.0.1",
	"permissions": ["https://steamcommunity.com/*", "https://*.steampowered.com/*", "https://market.csgo.com/*", "storage", "declarativeContent"],
	"background": {
		"scripts": ["scripts/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"128": "img/icon.png"
		}
	},
	"icons": {
		"128": "img/icon.png"
	}
}